<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel='stylesheet' href='/stylesheets/style.css' />
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
  <style>
    @import url('https://fonts.googleapis.com/css2?family=Poppins:wght@400;700&display=swap');
  </style>

  <script src="https://cdn.tailwindcss.com"></script>

</head>

<body class="bg-gray-100">
  <div class="grid grid-cols-5">
    <div class="col-span-1">

      <div class="h-screen pl-6 pt-8 pb-8 bg-gray-700  text-white sidebar">

        <div class="flex items-center mb-8">
          <!-- <div class="w-12 h-12 bg-zinc-400 rounded-full mr-4"></div> -->
          <img src="/images/logo-hmsi.png" alt="Logo HMSI" style="width: 170px; height: auto;">
          <div class="text-base font-normal font-['Poppins']"></div>
        </div>



        <div class="space-y-6">
          <div class="flex items-center active-sidebar">
            <div class="w-9 h-9  mr-4 "><i class="fa-solid fa-house "></i></i></div>
            <div class="text-base font-bold font-['Poppins'] sidebar-title"></div>
            <span class="active">Home</span>
          </div>


          <div class="flex items-center">
            <div class="w-8 h-8"><i class="fa-solid fa-briefcase"></i></div>
            <div class="text-base font-normal font-['Poppins'] sidebar-title "> Daftar
            </div>
          </div>

          <div class="flex items-center">
            <div class="w-8 h-8"><i class="fa-solid fa-users"></i></div>
            <div class="text-base font-normal font-['Poppins'] sidebar-title">Magang</div>
          </div>

          <div class="flex items-center">
            <div class="w-8 h-8"><i class="fa-solid fa-user"></i></div>
            <div class="text-base font-normal font-['Poppins'] sidebar-title">Profil</div>
          </div>

          <form action="/logout" method="POST"></form>
          <div class="flex items-center">
            <form action="/logout" method="POST"></form>
            <div class="w-8 h-8"><i class="fa-solid fa-right-from-bracket"></i></div>
            <div class="text-base font-normal font-['Poppins'] sidebar-title" onclick="window.location.href='/login'">
              Logout</div>
          </div>


          <script src="preline.js"></script>
        </div>
      </div>
    </div>
    <div class="col-span-4">

      <header class=" bg-gray-700 border-b border-gray-700 text-sm py-9 sm:py-9">
        <div class="me-8 md:me-8">
          <a class="flex-none text-xl font-semibold text-white" href="#" aria-label="Brand">Selamat Datang
            Di Laboratorium Dasar Komputasi</a>
        </div>

      </header>
      <!-- Isi konten di sini -->
      <p> </p>


      <!-- Table Section -->
      <div class="max-w-[95rem] px-4 py-10 sm:px-6 lg:px-8 lg:py-14 mx-auto">
        <!-- Card -->
        <div class="flex flex-col">
          <div class="-m-1.5 overflow-x-auto">
            <div class="p-1.5 min-w-full inline-block align-middle">
              <div class="bg-white border border-gray-200 rounded-xl shadow-sm overflow-hidden">

                <!-- Header -->
                <div class="px-6 py-4 grid gap-3 md:flex md:justify-between md:items-center border-b border-gray-200">
                  <div>
                    <h2 class="text-xl font-semibold text-gray-800">
                      Data Pendaftar
                    </h2>
                  </div>
                  <div>
                    <div class="inline-flex gap-x-2">


                    </div>
                  </div>
                </div>
                <!-- End Header -->


