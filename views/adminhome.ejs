                        <div class="flex items-center gap-x-2">
                          <span class="text-xs font-bold uppercase tracking-wide text-gray-950">
                            No
                          </span>
                        </div>
                      </th>

                      <th scope="col" class="px-6 py-3 text-start">
                        <div class="flex items-center gap-x-2">
                          <span class="text-xs font-bold uppercase tracking-wide text-gray-950">
                            Nama
                          </span>
                        </div>
