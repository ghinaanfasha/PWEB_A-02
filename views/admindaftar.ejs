<!DOCTYPE html>
<html>

<head>
  <title>
    <%= title %>
  </title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
</head>

<body class="font-poppins m-0 p-0">
  <!-- Partisi halaman -->
  <main class="grid grid-cols-7 grid-rows-3">
    <!-- Sidebar -->
    <aside class="col-span-1 row-span-3 bg-[#374353] h-screen p-4 sticky top-0">
      <div class="h-1/4 flex flex-col items-center justify-center mb-4">
        <img src="/images/logo-hmsi.png" alt="HMSI" class="w-40">
      </div>
      <div class="flex flex-col w-full">
        <button
          class="flex items-center text-[#e3ebf8] py-2 px-4 w-full transition-colors duration-300 cursor-pointer hover:bg-[#031227] hover:text-[#e3ebf8] rounded-md"
          onclick="window.location.href='/deskripsiadmin'">
          <i class="fas fa-home mr-4"></i> Home
        </button>
        <button
          class="flex items-center text-[#e3ebf8] py-2 px-4 w-full transition-colors duration-300 cursor-pointer hover:bg-[#031227] hover:text-[#e3ebf8] rounded-md"
          onclick="window.location.href='/admindaftar'">
          <i class="fas fa-list-alt mr-4"></i> Daftar
        </button>
        <button
          class="flex items-center text-[#e3ebf8] py-2 px-4 w-full transition-colors duration-300 cursor-pointer hover:bg-[#031227] hover:text-[#e3ebf8] rounded-md"
          onclick="window.location.href='/adminschedule'">
          <i class="fas fa-list-alt mr-4"></i> Magang
          <button
            class="flex items-center text-[#e3ebf8] py-2 px-4 w-full transition-colors duration-300 cursor-pointer hover:bg-[#031227] hover:text-[#e3ebf8] rounded-md"
            onclick="window.location.href='#'">
            <i class="fas fa-file-alt mr-4"></i> Penilaian
          </button>
          <button
            class="flex items-center text-[#e3ebf8] py-2 px-4 w-full transition-colors duration-300 cursor-pointer hover:bg-[#031227] hover:text-[#e3ebf8] rounded-md"
            onclick="window.location.href='/adminprofil'">
            <i class="fas fa-user mr-4"></i> Profil
          </button>
      </div>
    </aside>


    <!-- Akhir sidebar -->

    <!-- Konten -->
    <article class="col-span-6 row-span-3">
      <header class="bg-[#374353] flex items-center h-32">
        <h2 class="text-3xl text-white font-bold px-4">
          Magang Laboratory Dasar Komputasi</h2>
      </header>
      <div class="max-w-[95rem] px-4 py-10 sm:px-9 lg:px-8 lg:py-14 mx-auto">

        <!-- Card -->
        <div class="flex flex-col p-2">
          <div class="-m-1.5 overflow-x-auto">
            <div class="p-1.5 min-w-full inline-block align-middle">
              <div class="bg-white border border-gray-200 rounded-xl shadow-sm overflow-hidden">

                <!-- Header -->
                <div class="px-6 py-2 grid gap-3 md:flex md:justify-between md:items-center border-b border-gray-200">
                  <div>
                    <h2 class="text-xl font-semibold text-gray-800">
                      Data Pendaftar
                    </h2>
                  </div>
                </div>
                <!-- End Header -->


                <!-- Table -->
                <div class="flex flex-col items-center justify-center w-full pb-0">
                  <div class="overflow-x-scroll w-full max-w-4xl">
                    <div class="p-1.5 min-w-full align-middle">

                      <table class="min-w-full divide-y divide-gray-200 p-3">
                        <thead class="bg-gray-50">
                          <tr>
                            <th scope="col" class="px-6 py-3 text-start">
                              <div class="flex items-center gap-x-2">
                                <span class="text-xs font-bold uppercase tracking-wide text-gray-950">
                                  No
                                </span>
                              </div>
                            </th>

                            <th scope="col" class="px-6 py-3 text-start">
                              <div class="flex items-center gap-x-2">
                                <span class="text-xs font-bold uppercase tracking-wide text-gray-950">
                                  Nama
                                </span>
                              </div>
                            </th>

                            <th scope="col" class="px-6 py-3 text-start">
                              <div class="flex items-center gap-x-2">
                                <span class="text-xs font-bold uppercase tracking-wide text-gray-950">
                                  Nim
                                </span>
                              </div>
                            </th>


                            <th scope="col" class="px-6 py-3 text-start">
                              <div class="flex items-center gap-x-2">
                                <span class="text-xs font-bold uppercase tracking-wide text-gray-950">
                                  Alasan Bergabung
                                </span>
                              </div>
                            </th>

                            <th scope="col" class="px-6 py-3 text-start">
                              <div class="flex items-center gap-x-2">
                                <span class="text-xs font-bold uppercase tracking-wide text-gray-950">
                                  CV
                                </span>
                              </div>
                            </th>

                            <th scope="col" class="px-6 py-3 text-start">
                              <div class="flex items-center gap-x-2">
                                <span class="text-xs font-bold uppercase tracking-wide text-gray-950">
                                  Surat Komitmen
                                </span>
                              </div>
                            </th>

                            <th scope="col" class="px-6 py-3 text-start">
                              <div class="flex items-center gap-x-2">
                                <span class="text-xs font-bold uppercase tracking-wide text-gray-950">
                                  Verifikasi
                                </span>
                              </div>
                            </th>

                            <th scope="col" class="px-6 py-3 text-end"></th>
                          </tr>
                        </thead>

                        <tbody class="divide-y divide-gray-200">

                          <% pendaftar.forEach((item, index)=> { %>

                            <tr class="bg-white hover:bg-gray-50">
                              <td class="px-6 py-2"><span class="text-sm text-gray-600">
                                  <%= index + 1 %>
                                </span></td>
                              <td class="px-6 py-2"><span class="text-sm text-gray-600">
                                  <%= item.user.first_name %>
                                    <%= item.user.last_name %>
                                </span></td>
                              <td class="px-6 py-2"><span class="text-sm text-gray-600">
                                  <%= item.user.nim %>
                                </span></td>
                              <td class="px-6 py-2"><span class="text-sm text-gray-600">
                                  <%= item.alasan_bergabung %>
                                </span></td>
                              <td class="size-px whitespace-nowrap">
                                <span class="px-6 py-1.5">

                                  <a href="<%= item.cv %>" target="_blank">




                                    <span
                                      class="py-1 px-2 inline-flex justify-center items-center gap-2 rounded-lg border font-medium bg-white text-gray-700 shadow-sm align-middle hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-offset-white focus:ring-blue-600 transition-all text-sm">
                                      <svg class="flex-shrink-0 size-4" xmlns="http://www.w3.org/2000/svg" width="16"
                                        height="16" fill="currentColor" viewBox="0 0 16 16">
                                        <path
                                          d="M1.92.506a.5.5 0 0 1 .434.14L3 1.293l.646-.647a.5.5 0 0 1 .708 0L5 1.293l.646-.647a.5.5 0 0 1 .708 0L7 1.293l.646-.647a.5.5 0 0 1 .708 0L9 1.293l.646-.647a.5.5 0 0 1 .708 0l.646.647.646-.647a.5.5 0 0 1 .708 0l.646.647.646-.647a.5.5 0 0 1 .801.13l.5 1A.5.5 0 0 1 15 2v12a.5.5 0 0 1-.053.224l-.5 1a.5.5 0 0 1-.8.13L13 14.707l-.646.647a.5.5 0 0 1-.708 0L11 14.707l-.646.647a.5.5 0 0 1-.708 0L9 14.707l-.646.647a.5.5 0 0 1-.708 0L7 14.707l-.646.647a.5.5 0 0 1-.708 0L5 14.707l-.646.647a.5.5 0 0 1-.708 0L3 14.707l-.646.647a.5.5 0 0 1-.801-.13l-.5-1A.5.5 0 0 1 1 14V2a.5.5 0 0 1 .053-.224l.5-1a.5.5 0 0 1 .367-.27zm.217 1.338L2 2.118v11.764l.137.274.51-.51a.5.5 0 0 1 .707 0l.646.647.646-.646a.5.5 0 0 1 .708 0l.646.646.646-.646a.5.5 0 0 1 .708 0l.646.646.646-.646a.5.5 0 0 1 .708 0l.646.646.646-.646a.5.5 0 0 1 .708 0l.646.646.646-.646a.5.5 0 0 1 .708 0l.509.509.137-.274V2.118l-.137-.274-.51.51a.5.5 0 0 1-.707 0L12 1.707l-.646.647a.5.5 0 0 1-.708 0L10 1.707l-.646.647a.5.5 0 0 1-.708 0L8 1.707l-.646.647a.5.5 0 0 1-.708 0L6 1.707l-.646.647a.5.5 0 0 1-.708 0L4 1.707l-.646.647a.5.5 0 0 1-.708 0l-.509-.51z" />
                                        <path
                                          d="M3 4.5a.5.5 0 0 1 .5-.5h6a.5.5 0 1 1 0 1h-6a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h6a.5.5 0 1 1 0 1h-6a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h6a.5.5 0 1 1 0 1h-6a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h6a.5.5 0 0 1 0 1h-6a.5.5 0 0 1-.5-.5zm8-6a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 0 1h-1a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 0 1h-1a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 0 1h-1a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 0 1h-1a.5.5 0 0 1-.5-.5z" />
                                      </svg>
                                      View
                                    </span>
                                  </a>
                                </span>
                              </td>

                              <td class="size-px whitespace-nowrap">

                                <span class="px-6 py-1.5">
                                  <a href="<%= item.surat_komitmen %>" target="_blank">
                                    <span
                                      class="py-1 px-2 inline-flex justify-center items-center gap-2 rounded-lg border font-medium bg-white text-gray-700 shadow-sm align-middle hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-offset-white focus:ring-blue-600 transition-all text-sm">
                                      <svg class="flex-shrink-0 size-4" xmlns="http://www.w3.org/2000/svg" width="16"
                                        height="16" fill="currentColor" viewBox="0 0 16 16">
                                        <path
                                          d="M1.92.506a.5.5 0 0 1 .434.14L3 1.293l.646-.647a.5.5 0 0 1 .708 0L5 1.293l.646-.647a.5.5 0 0 1 .708 0L7 1.293l.646-.647a.5.5 0 0 1 .708 0L9 1.293l.646-.647a.5.5 0 0 1 .708 0l.646.647.646-.647a.5.5 0 0 1 .708 0l.646.647.646-.647a.5.5 0 0 1 .801.13l.5 1A.5.5 0 0 1 15 2v12a.5.5 0 0 1-.053.224l-.5 1a.5.5 0 0 1-.8.13L13 14.707l-.646.647a.5.5 0 0 1-.708 0L11 14.707l-.646.647a.5.5 0 0 1-.708 0L9 14.707l-.646.647a.5.5 0 0 1-.708 0L7 14.707l-.646.647a.5.5 0 0 1-.708 0L5 14.707l-.646.647a.5.5 0 0 1-.708 0L3 14.707l-.646.647a.5.5 0 0 1-.801-.13l-.5-1A.5.5 0 0 1 1 14V2a.5.5 0 0 1 .053-.224l.5-1a.5.5 0 0 1 .367-.27zm.217 1.338L2 2.118v11.764l.137.274.51-.51a.5.5 0 0 1 .707 0l.646.647.646-.646a.5.5 0 0 1 .708 0l.646.646.646-.646a.5.5 0 0 1 .708 0l.646.646.646-.646a.5.5 0 0 1 .708 0l.646.646.646-.646a.5.5 0 0 1 .708 0l.646.646.646-.646a.5.5 0 0 1 .708 0l.509.509.137-.274V2.118l-.137-.274-.51.51a.5.5 0 0 1-.707 0L12 1.707l-.646.647a.5.5 0 0 1-.708 0L10 1.707l-.646.647a.5.5 0 0 1-.708 0L8 1.707l-.646.647a.5.5 0 0 1-.708 0L6 1.707l-.646.647a.5.5 0 0 1-.708 0L4 1.707l-.646.647a.5.5 0 0 1-.708 0l-.509-.51z" />
                                        <path
                                          d="M3 4.5a.5.5 0 0 1 .5-.5h6a.5.5 0 1 1 0 1h-6a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h6a.5.5 0 1 1 0 1h-6a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h6a.5.5 0 1 1 0 1h-6a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h6a.5.5 0 0 1 0 1h-6a.5.5 0 0 1-.5-.5zm8-6a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 0 1h-1a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 0 1h-1a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 0 1h-1a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 0 1h-1a.5.5 0 0 1-.5-.5z" />
                                      </svg>
                                      View
                                    </span>
                                  </a>
                                </span>
                              </td>

                              <td class="size-px whitespace-nowrap">
                                <!-- Jika item.status = 0 -->
                                <% if (item.verifikasi===0) { %>
                                  <a class="block" href="#"
                                    onclick="event.preventDefault(); acceptUser('<%= item.id_user %>');">
                                    <span class="block px-6 py-2">
                                      <span
                                        class="py-1 px-1.5 inline-flex items-center gap-x-1 text-xs font-medium bg-teal-100 text-teal-800 rounded-full">
                                        <svg class="size-2.5" xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                          fill="currentColor" viewBox="0 0 16 16">
                                          <path
                                            d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-3.97-3.03a.75.75 0 0 0-1.08.022L7.477 9.417 5.384 7.323a.75.75 0 0 0-1.06 1.06L6.97 11.03a.75.75 0 0 0 1.079-.02l3.992-4.99a.75.75 0 0 0-.01-1.05z" />
                                        </svg>
                                        Verified
                                      </span>
                                    </span>
                                  </a>
                                  <a class="block p-6" href="#"
                                    onclick="event.preventDefault(); rejectUser('<%= item.id_user %>');">
                                    <span
                                      class="py-1 px-1.5 inline-flex items-center gap-x-1 text-xs font-medium bg-red-100 text-red-800 rounded-full dark:bg-red-500/10 dark:text-red-500">
                                      <svg class="size-2.5" xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                        fill="currentColor" viewBox="0 0 16 16">
                                        <path
                                          d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zM5.354 4.646a.5.5 0 1 0-.708.708L7.293 8l-2.647 2.646a.5.5 0 0 0 .708.708L8 8.707l2.646 2.647a.5.5 0 0 0 .708-.708L8.707 8l2.647-2.646a.5.5 0 0 0-.708-.708L8 7.293 5.354 4.646z" />
                                      </svg>
                                      Rejected
                                    </span>
                                  </a>

                                  <% } else if (item.verifikasi===1) { %>
                                    <span class="block px-6 py-2">
                                      <span
                                        class="py-1 px-1.5 inline-flex items-center gap-x-1 text-xs font-medium bg-teal-100 text-teal-800 rounded-full">
                                        <svg class="size-2.5" xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                          fill="currentColor" viewBox="0 0 16 16">
                                          <path
                                            d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-3.97-3.03a.75.75 0 0 0-1.08.022L7.477 9.417 5.384 7.323a.75.75 0 0 0-1.06 1.06L6.97 11.03a.75.75 0 0 0 1.079-.02l3.992-4.99a.75.75 0 0 0-.01-1.05z" />
                                        </svg>
                                        Verified
                                      </span>
                                    </span>
                                    <% } else if (item.verifikasi===2) { %>
                                      <span class="block p-6">
                                        <span
                                          class="py-1 px-1.5 inline-flex items-center gap-x-1 text-xs font-medium bg-red-100 text-red-800 rounded-full dark:bg-red-500/10 dark:text-red-500">
                                          <svg class="size-2.5" xmlns="http://www.w3.org/2000/svg" width="16"
                                            height="16" fill="currentColor" viewBox="0 0 16 16">
                                            <path
                                              d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zM5.354 4.646a.5.5 0 1 0-.708.708L7.293 8l-2.647 2.646a.5.5 0 0 0 .708.708L8 8.707l2.646 2.647a.5.5 0 0 0 .708-.708L8.707 8l2.647-2.646a.5.5 0 0 0-.708-.708L8 7.293 5.354 4.646z" />
                                          </svg>
                                          Rejected
                                        </span>
                                      </span>
                                      <% } %>
                              </td>

                            </tr>
                            <% }); %>
                        </tbody>
                      </table>
                    </div>



                    <!-- End Footer -->
                  </div>
                </div>


                <!-- Modal -->
                <div id="hs-ai-invoice-modal"
                  class="hs-overlay hidden size-full fixed top-0 start-0 z-[80] overflow-x-hidden overflow-y-auto pointer-events-none">
                  <div
                    class="hs-overlay-open:mt-7 hs-overlay-open:opacity-100 hs-overlay-open:duration-500 mt-0 opacity-0 ease-out transition-all sm:max-w-lg sm:w-full m-3 sm:mx-auto">
                    <div class="relative flex flex-col bg-white shadow-lg rounded-xl pointer-events-auto">
                      <div class="relative overflow-hidden min-h-32 bg-gray-900 text-center rounded-t-xl">
                        <!-- Close Button -->
                        <div class="absolute top-2 end-2">
                          <button type="button"
                            class="inline-flex flex-shrink-0 justify-center items-center size-8 rounded-lg text-gray-500 hover:text-gray-400 focus:outline-none focus:ring-2 focus:ring-gray-400 focus:ring-offset-2 focus:ring-offset-gray-900 transition-all text-sm"
                            data-hs-overlay="#hs-ai-invoice-modal">
                            <span class="sr-only">Close</span>
                            <svg class="flex-shrink-0 size-4" xmlns="http://www.w3.org/2000/svg" width="24" height="24"
                              viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"
                              stroke-linecap="round" stroke-linejoin="round">
                              <path d="M18 6 6 18" />
                              <path d="m6 6 12 12" />
                            </svg>
                          </button>
                        </div>
                        <!-- End Close Button -->

                        <!-- SVG Background Element -->
                        <figure class="absolute inset-x-0 bottom-0">
                          <svg preserveAspectRatio="none" xmlns="http://www.w3.org/2000/svg" x="0px" y="0px"
                            viewBox="0 0 1920 100.1">
                            <path fill="currentColor" class="fill-white" d="M0,0c0,0,934.4,93.4,1920,0v100.1H0L0,0z">
                            </path>
                          </svg>
                        </figure>
                        <!-- End SVG Background Element -->
                      </div>

                      <div class="relative z-10 -mt-12">
                        <!-- Icon -->
                        <span
                          class="mx-auto flex justify-center items-center size-[62px] rounded-full border border-gray-200 bg-white text-gray-700 shadow-sm">
                          <svg class="size-6" xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                            fill="currentColor" viewBox="0 0 16 16">
                            <path
                              d="M1.92.506a.5.5 0 0 1 .434.14L3 1.293l.646-.647a.5.5 0 0 1 .708 0L5 1.293l.646-.647a.5.5 0 0 1 .708 0L7 1.293l.646-.647a.5.5 0 0 1 .708 0L9 1.293l.646-.647a.5.5 0 0 1 .708 0l.646.647.646-.647a.5.5 0 0 1 .708 0l.646.647.646-.647a.5.5 0 0 1 .801.13l.5 1A.5.5 0 0 1 15 2v12a.5.5 0 0 1-.053.224l-.5 1a.5.5 0 0 1-.8.13L13 14.707l-.646.647a.5.5 0 0 1-.708 0L11 14.707l-.646.647a.5.5 0 0 1-.708 0L9 14.707l-.646.647a.5.5 0 0 1-.708 0L7 14.707l-.646.647a.5.5 0 0 1-.708 0L5 14.707l-.646.647a.5.5 0 0 1-.708 0L3 14.707l-.646.647a.5.5 0 0 1-.801-.13l-.5-1A.5.5 0 0 1 1 14V2a.5.5 0 0 1 .053-.224l.5-1a.5.5 0 0 1 .367-.27zm.217 1.338L2 2.118v11.764l.137.274.51-.51a.5.5 0 0 1 .707 0l.646.647.646-.646a.5.5 0 0 1 .708 0l.646.646.646-.646a.5.5 0 0 1 .708 0l.646.646.646-.646a.5.5 0 0 1 .708 0l.646.646.646-.646a.5.5 0 0 1 .708 0l.646.646.646-.646a.5.5 0 0 1 .708 0l.509.509.137-.274V2.118l-.137-.274-.51.51a.5.5 0 0 1-.707 0L12 1.707l-.646.647a.5.5 0 0 1-.708 0L10 1.707l-.646.647a.5.5 0 0 1-.708 0L8 1.707l-.646.647a.5.5 0 0 1-.708 0L6 1.707l-.646.647a.5.5 0 0 1-.708 0L4 1.707l-.646.647a.5.5 0 0 1-.708 0l-.509-.51z" />
                            <path
                              d="M3 4.5a.5.5 0 0 1 .5-.5h6a.5.5 0 1 1 0 1h-6a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h6a.5.5 0 1 1 0 1h-6a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h6a.5.5 0 1 1 0 1h-6a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h6a.5.5 0 0 1 0 1h-6a.5.5 0 0 1-.5-.5zm8-6a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 0 1h-1a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 0 1h-1a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 0 1h-1a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 0 1h-1a.5.5 0 0 1-.5-.5z" />
                          </svg>
                        </span>
                        <!-- End Icon -->
                      </div>
                    </div>
                  </div>
                </div>



                <!-- disini -->


              </div>
            </div>
            <!-- End Card -->
          </div>
          <!-- End Table Section -->


          <!-- ========== MAIN CONTENT ========== -->

          <!-- ========== END MAIN CONTENT ========== -->
        </div>




        <div class="p-1.5 min-w-full inline-block align-middle">
          <div class="bg-white border border-gray-200 rounded-xl shadow-sm overflow-hidden">

            <!-- Header -->
            <div class="px-6 py-4 grid gap-3 md:flex md:justify-between md:items-center border-b border-gray-200">
              <div>
                <h2 class="text-xl font-semibold text-gray-800">
                  Jadwalkan Wawancara
                </h2>
              </div>
              <div>
                <div class="inline-flex gap-x-2">


                </div>
              </div>
            </div>
            <!-- End Header -->


            <!-- Table -->
            <table class="min-w-full divide-y divide-gray-200">
              <thead class="bg-gray-50">
                <tr>
                  <th scope="col" class="px-6 py-3 text-start">
                    <div class="flex items-center gap-x-2">
                      <span class="text-xs font-bold uppercase tracking-wide text-gray-950">
                        No
                      </span>
                    </div>
                  </th>

                  <th scope="col" class="px-6 py-3 text-start">
                    <div class="flex items-center gap-x-2">
                      <span class="text-xs font-bold uppercase tracking-wide text-gray-950">
                        Nama
                      </span>
                    </div>
                  </th>

                  <th scope="col" class="px-6 py-3 text-start">
                    <div class="flex items-center gap-x-2">
                      <span class="text-xs font-bold uppercase tracking-wide text-gray-950">
                        Nim
                      </span>
                    </div>
                  </th>



                  <th scope="col" class="px-6 py-3 text-start">
                    <div class="flex items-center gap-x-2">
                      <span class="text-xs font-bold uppercase tracking-wide text-gray-950">
                        Tanggal
                      </span>
                    </div>
                  </th>

                  <th scope="col" class="px-6 py-3 text-start">
                    <div class="flex items-center gap-x-2">
                      <span class="text-xs font-bold uppercase tracking-wide text-gray-950">
                        Waktu
                      </span>
                    </div>
                  </th>

                  <th scope="col" class="px-6 py-3 text-start">
                    <div class="flex items-center gap-x-2">
                      <span class="text-xs font-bold uppercase tracking-wide text-gray-950">
                        Pengajuan
                      </span>
                    </div>
                  </th>

                  <th scope="col" class="px-6 py-3 text-start">
                    <div class="flex items-center gap-x-2">
                      <span class="text-xs font-bold uppercase tracking-wide text-gray-950">
                        Persetujuan Jadwal
                      </span>
                    </div>
                  </th>


                  <th scope="col" class="px-6 py-3 text-start">
                    <div class="flex items-center gap-x-2">
                      <span class="text-xs font-bold uppercase tracking-wide text-gray-950">
                        Penilaian
                      </span>
                    </div>
                  </th>

                  <th scope="col" class="px-6 py-3 text-end"></th>
                </tr>
              </thead>


              <tbody class="divide-y divide-gray-200">


                <% wawancara.forEach(item=> { %>
                  <tr class="bg-white hover:bg-gray-50">

                    <td class="size-px whitespace-nowrap">
                      <button type="button" class="block" data-hs-overlay="#hs-ai-invoice-modal">
                        <span class="block px-6 py-2">
                          <span class="text-sm text-gray-600">
                            <%= item.id %>
                          </span>
                        </span>
                      </button>
                    </td>

                    <td class="size-px whitespace-nowrap">
                      <button type="button" class="block" data-hs-overlay="#hs-ai-invoice-modal">
                        <span class="block px-6 py-2">
                          <span class="text-sm text-gray-600">
                            <%= item.user.first_name %>
                              <%= item.user.last_name %>
                          </span>
                        </span>
                      </button>
                    </td>

                    <td class="size-px whitespace-nowrap">
                      <button type="button" class="block" data-hs-overlay="#hs-ai-invoice-modal">
                        <span class="block px-6 py-2">
                          <span class="text-sm text-gray-600">
                            <%= item.user.nim %>
                          </span>
                        </span>
                      </button>
                    </td>


                    <td class="size-px whitespace-nowrap">
                      <button type="button" class="block" data-hs-overlay="#hs-ai-invoice-modal">
                        <span class="block px-6 py-2">
                          <span class="text-sm text-gray-600">
                            <%= item.pengajuan_tgl %>
                          </span>
                        </span>
                      </button>
                    </td>

                    <td class="size-px whitespace-nowrap">
                      <button type="button" class="block" data-hs-overlay="#hs-ai-invoice-modal">
                        <span class="block px-6 py-2">
                          <span class="text-sm text-gray-600">
                            <%= item.pengajuan_waktu %>
                          </span>
                        </span>
                      </button>
                    </td>

                   

                    <td class="size-px whitespace-nowrap">
                      <!-- Jika item.status = 0 -->
                      <% if (item.persetujuan===0) { %>
                        <a class="block p-6" href="#"
                          onclick="event.preventDefault(); rejectUser('<%= item.id_user %>');">
                          <span
                            class="py-1 px-1.5 inline-flex items-center gap-x-1 text-xs font-medium bg-red-100 text-red-800 rounded-full dark:bg-red-500/10 dark:text-red-500">
                            <svg class="size-2.5" xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                              fill="currentColor" viewBox="0 0 16 16">
                              <path
                                d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zM5.354 4.646a.5.5 0 1 0-.708.708L7.293 8l-2.647 2.646a.5.5 0 0 0 .708.708L8 8.707l2.646 2.647a.5.5 0 0 0 .708-.708L8.707 8l2.647-2.646a.5.5 0 0 0-.708-.708L8 7.293 5.354 4.646z" />
                            </svg>
                            Rejected
                          </span>
                        </a>

                        <% } else if (item.persetujuan===1) { %>
                          <span class="block px-6 py-2">
                            <span
                              class="py-1 px-1.5 inline-flex items-center gap-x-1 text-xs font-medium bg-teal-100 text-teal-800 rounded-full">
                              <svg class="size-2.5" xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                fill="currentColor" viewBox="0 0 16 16">
                                <path
                                  d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-3.97-3.03a.75.75 0 0 0-1.08.022L7.477 9.417 5.384 7.323a.75.75 0 0 0-1.06 1.06L6.97 11.03a.75.75 0 0 0 1.079-.02l3.992-4.99a.75.75 0 0 0-.01-1.05z" />
                              </svg>
                              Verified
                            </span>
                          </span>
                          <% }%>
                    </td>

                    <td class="size-px whitespace-nowrap">
                      <button type="button" class="block" data-hs-overlay="#hs-ai-invoice-modal">
                        <span class="block px-6 py-2">
                          <span class="text-sm text-gray-600">
                            <%= item.user.waktu %>
                          </span>
                        </span>
                      </button>
                    </td>
                  </tr>

                  <% }) %>


              </tbody>
            </table>



            <!-- End Footer -->
          </div>
        </div>

      </div>
    </article>
    <!-- Akhir Konten -->

    <script src="https://cdn.tailwindcss.com"></script>
    <script>
      async function rejectUser(userId) {
        try {
          const url = `/admindaftar/tolak/${userId}`;

          const response = await fetch(url, {
            method: 'POST',
            headers: {
              'Content-Type': 'application/json'
            },
            body: JSON.stringify({})
          });

          if (!response.ok) {
            const errorData = await response.json();
            throw new Error(`Error: ${errorData.msg}`);
          }

          // Handle success, e.g., display a success message
          alert('User rejected successfully');
          location.reload()
        } catch (error) {
          console.error('There was a problem rejecting the user:', error);
          // Handle errors, e.g., display an error message
          alert(error.message);
        }
      }

      async function acceptUser(userId) {
        try {
          const url = `/admindaftar/terima/${userId}`;

          const response = await fetch(url, {
            method: 'POST',
            headers: {
              'Content-Type': 'application/json'
            },
            body: JSON.stringify({})
          });

          if (!response.ok) {
            const errorData = await response.json();
            throw new Error(`Error: ${errorData.msg}`);
          }

          // Handle success, e.g., display a success message
          alert('User accepted successfully');
          location.reload()
        } catch (error) {
          console.error('There was a problem rejecting the user:', error);
          // Handle errors, e.g., display an error message
          alert(error.message);
        }
      }
    </script>
  </main>
</body>

</html>
<!-- Akhir partisi -->